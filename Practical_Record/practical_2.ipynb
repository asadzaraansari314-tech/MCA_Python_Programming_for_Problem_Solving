   "source": [
    "# Program to count vowels in a string\n",
    "\n",
    "string = input(\"Enter a string: \")\n",
    "count = 0\n",
    "\n",
    "for ch in string:\n",
    "    if ch in \"aeiouAEIOU\":\n",
    "        count += 1\n",
    "\n",
    "print(\"Number of vowels:\", count)\n"
   ]
